  "CODE_SNIPPETS": {
    "jud_specific_neevaluable_check": {
      "_comment": "Fragment din get_clarification_needs: Verifică dacă obiectul ECRIS corespunde unor cazuri specifice Art. 94 NCPC considerate de regulă neevaluabile, pentru a decide dacă solicitarea valorii este necesară.",
      "code": "is_clearly_non_evaluable_jud = is_family_suggested or is_inheritance_suggested or \\\n                      (\"grănițuire\" in obj_words) or \\\n                      (\"granituire\" in obj_words) or \\\n                      obj_words.intersection({\"declarare moarte\", \"declarare judecătorească moarte\", \"declarare judecatoreasca moarte\"}) or \\\n                      obj_words.intersection({\"stare civilă\", \"stare civila\", \"rectificare act\"}) or \\\n                      obj_words.intersection({\"asociație proprietari\", \"asociatie proprietari\"}) or \\\n                      obj_words.intersection({\"obligație de a face\", \"obligatie de a face\", \"obligație de a nu face\", \"obligatie de a nu face\"})"
    },
    "material_competence_declarare_moarte": {
      "_comment": "Fragment din determine_competence: Stabilește competența materială la Judecătorie pentru cazurile de declarare judecătorească a morții (Art. 94 pct. 1 lit. i NCPC).",
      "code": "elif obj_words.intersection({\"declarare judecătorească moarte\", \"declarare judecatoreasca moarte\", \"declarare moarte\"}):\n    material_level = \"Judecătorie\"; material_basis = \"Art. 94 pct. 1 lit. i) NCPC\""
    },
    "material_competence_stare_civila": {
      "_comment": "Fragment din determine_competence: Stabilește competența materială la Judecătorie pentru cererile de stare civilă, inclusiv rectificări (Art. 94 pct. 1 lit. b NCPC și Dec. ICCJ 28/2021).",
      "code": "elif obj_words.intersection({\"stare civilă\", \"stare civila\", \"rectificare act\"}):\n    material_level = \"Judecătorie\"; material_basis = \"Art. 94 pct. 1 lit. b) NCPC / Dec. ICCJ 28/2021\""
    },
    "material_competence_asociatie_proprietari": {
      "_comment": "Fragment din determine_competence: Stabilește competența materială la Judecătorie pentru cererile formulate de/împotriva asociațiilor de proprietari (Art. 94 pct. 1 lit. c NCPC).",
      "code": "elif obj_words.intersection({\"asociație proprietari\", \"asociatie proprietari\", \"administrare clădiri\", \"administrare cladiri\"}):\n    material_level = \"Judecătorie\"; material_basis = \"Art. 94 pct. 1 lit. c) NCPC\""
    },
    "territorial_competence_locatiune": {
      "_comment": "Fragment din determine_competence: Identifică o posibilă instanță competentă teritorial (alternativă) la locul situării imobilului pentru litigiile privind locațiunea/închirierea/arendarea (Art. 113 pct. 4 NCPC).",
      "code": "if obj_words.intersection({\"locațiune\", \"locatiune\", \"închiriere\", \"inchiriere\", \"arendare\"}) and clarifications.property_location:\n    court = _find_competent_court_by_location(material_level, clarifications.property_location, \"Competență alternativă (Art. 113 pct. 4 NCPC - Locațiune imobil)\")\n    add_potential_court(court)"
    },
    "territorial_competence_tabulara": {
      "_comment": "Fragment din determine_competence: Identifică o posibilă instanță competentă teritorial (alternativă) la locul situării imobilului pentru acțiunile tabulare (carte funciară) (Art. 113 pct. 5 NCPC).",
      "code": "if obj_words.intersection({\"tabulară\", \"tabulara\", \"carte funciară\", \"carte funciara\", \"intabulare\", \"rectificare cf\"}) and clarifications.property_location:\n    court = _find_competent_court_by_location(material_level, clarifications.property_location, \"Competență alternativă (Art. 113 pct. 5 NCPC - Acțiune tabulară)\")\n    add_potential_court(court)"
    },
    "territorial_competence_ocrotire": {
      "_comment": "Fragment din determine_competence: Identifică o posibilă instanță competentă teritorial (alternativă) la domiciliul persoanei ocrotite pentru cazurile de tutelă, curatelă, interdicție (Art. 114 NCPC).",
      "code": "elif obj_words.intersection({\"tutelă\", \"tutela\", \"curatelă\", \"curatela\", \"ocrotire\", \"interdicție\", \"interdictie\"}) and clarifications.protected_person_domicile:\n    court = _find_competent_court_by_location(material_level, clarifications.protected_person_domicile, \"Competență alternativă (Art. 114 NCPC - Domiciliu ocrotit)\")\n    add_potential_court(court)"
    }
